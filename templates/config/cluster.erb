
cluster <%= @cluster_name %>
<% if @forward_proto == "file" -%>
  <%= @forward_proto %> <%= @file %>
<% elsif @forward_proto == "forward" -%>
  <%= @forward_proto %> <% if @replication_factor.to_i > 1 -%>replication <%= @replication_factor %><% end %>
  <% @hosts.each do | host | -%>
    <%= #{host['ip']} %>:<%= #{host['port']} %><% if #{host['proto']} %><%= #{host['proto']} %><% end %>
  <% end -%>
<% end -%>
  ;